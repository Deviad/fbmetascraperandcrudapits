{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist",
        "noImplicitAny": false,
        "noEmitHelpers": true,
        "sourceMap": true,
        "noEmitOnError": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "es6",
            "dom"
        ]
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ],
    "types": [
        "rx","express", "node"
    ]
}